{
	"name": "david63/sitelogo",
	"type": "phpbb-extension",
	"description": "Replace the standard phpBB logo with own custom logo.",
	"version": "2.1.0-RC6",
	"time": "2019-05-12",
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "David Wood",
			"role": "Developer"
		},
		{
            "name": "Alessandro DI BENEDETTO",
            "email": "alex@phpbb-store.it",
            "homepage": "http://phpbb-store.it",
            "role": "Developer - Background image"
        },
		{
            "name": "Jan Odvarko",
            "homepage": "http://odvarko.cz",
            "role": "Creator - jscolor, JavaScript Color Picker"
        }
    ],
	"require": {
        "php": ">=5.4.0"
    },
	"extra": {
		"display-name": "[*] Site logo",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.0"
		},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/3.2",
        	"filename": "/site_logo_version_file.json"
    	}
	}
}
